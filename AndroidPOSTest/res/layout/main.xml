<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" android:gravity="fill">
    
    <ImageButton android:id="@+id/one"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignParentLeft="true"
    		android:layout_marginTop="10px"
    		android:layout_marginLeft="5px"
    		android:src="@drawable/icon_product" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:layout_marginLeft="25px"
    		  android:id="@+id/txtProduct" 
    		  android:text="Products"     		 
    		  android:layout_below="@+id/one"  />
    		
    <ImageButton android:id="@+id/two"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignTop="@id/one"
    		android:layout_toRightOf="@id/one"
    		android:layout_marginLeft="5px"
    		android:textSize="25px"
    		android:src="@drawable/icon_stock" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content"
    		  android:id="@+id/txtStocks" 
    		  android:text="Stocks"     		  
    		  android:layout_below="@id/two" 
    		  android:layout_marginLeft="30px"
    		  android:layout_alignLeft="@id/two" />
    		
    <ImageButton android:id="@+id/three"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignTop="@id/two"
    		android:layout_toRightOf="@id/two"
    		android:layout_marginLeft="5px"
    		android:padding="0px"    		
    		android:src="@drawable/icon_sales" />
    		
    <TextView android:layout_width="wrap_content" 
    	      android:layout_height="wrap_content" 
    	      android:text="Sales" android:id="@+id/txtSales" 
    	      android:layout_below="@+id/three" 
    	      android:layout_alignLeft="@+id/three" 
    		  android:layout_marginLeft="30px"
    		  android:layout_alignRight="@+id/three"/>
    		
    <ImageButton android:id="@+id/four"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignParentLeft="true"
    		android:layout_below="@id/one"
    		android:layout_marginLeft="5px"
    		android:layout_marginTop="25px"    		
    		android:src="@drawable/icon_utilities" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:text="Utilities" 
    		  android:id="@+id/txtUtilities" 
    		  android:layout_below="@+id/four" 
    		  android:layout_alignLeft="@+id/four" 
    		  android:layout_marginLeft="25px"
    		  android:layout_alignRight="@+id/four"/>
    		
    <ImageButton android:id="@+id/five"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignTop="@id/four"
    		android:layout_toRightOf="@id/four"
    		android:layout_marginLeft="5px"
    		android:textSize="25px"
    		android:src="@drawable/icon_user" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:text="Users" android:id="@+id/txtUsers" 
    		  android:layout_below="@+id/five" 
    		  android:layout_alignLeft="@+id/five" 
    		  android:layout_marginLeft="30px"
    		  android:layout_alignRight="@+id/five"/>
    		
    <ImageButton android:id="@+id/six"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignTop="@id/five"
    		android:layout_toRightOf="@id/five"
    		android:layout_marginLeft="5px"
    		android:textSize="25px"
    		android:src="@drawable/icon_zone" />
    		
    <TextView android:layout_width="wrap_content" 
    		android:layout_height="wrap_content" 
    		android:text="Zones" 
    		android:id="@+id/txtZones" 
    		android:layout_below="@+id/six" 
    		android:layout_alignLeft="@+id/six" 
    		android:layout_marginLeft = "30px"
    		android:layout_alignRight="@+id/six"/>
    		
    <ImageButton android:id="@+id/seven"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignParentLeft="true"
    		android:layout_below="@id/four"
    		android:layout_marginTop="25px"
    		android:layout_marginLeft="5px"
    		android:textSize="25px"
    		android:src="@drawable/icon_cash" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:text="Cash Mng" 
    		  android:layout_below="@+id/seven" 
    		  android:id="@+id/txtCash" 
    		  android:layout_alignLeft="@+id/seven"
    		  android:layout_marginLeft="20px" 
    		  android:layout_alignRight="@+id/seven"/>
    		
    <ImageButton android:id="@+id/eight"
    		android:layout_height="100px"
    		android:layout_width="100px"
    		android:layout_alignTop="@id/seven"
    		android:layout_toRightOf="@id/seven"
    		android:layout_marginLeft="5px"
    		android:textSize="25px"
    		android:src="@drawable/icon_inventory" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:text="Stocks" 
    		  android:layout_below="@+id/eight" 
    		  android:id="@+id/txtStocks" 
    		  android:layout_alignLeft="@+id/eight" 
    		  android:layout_marginLeft="30px"
    		  android:layout_alignRight="@+id/eight"/>    		 
    		
    <ImageButton android:id="@+id/nine"
	    		android:layout_height="100px"
	    		android:layout_width="100px"
	    		android:layout_alignTop="@id/eight"
	    		android:layout_toRightOf="@id/eight"
	    		android:layout_marginLeft="5px"
	    		android:textSize="25px"
	    		android:src="@drawable/icon_help" />
    		
    <TextView android:layout_width="wrap_content" 
    		  android:layout_height="wrap_content" 
    		  android:text="Help" 
    		  android:layout_below="@+id/nine" 
    		  android:id="@+id/txtHelp" 
    		  android:layout_alignLeft="@+id/nine" 
    		  android:layout_marginLeft="35px"
    		  android:layout_alignRight="@+id/nine"/>
    
</RelativeLayout>
